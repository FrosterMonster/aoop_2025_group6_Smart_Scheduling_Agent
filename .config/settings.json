{
  "app_name": "AI Schedule Agent",
  "app_version": "1.0.0",
  "google_calendar": {
    "scopes": [
      "https://www.googleapis.com/auth/calendar"
    ],
    "timezone": "UTC"
  },
  "ui": {
    "window_width": 1200,
    "window_height": 800,
    "theme": "default"
  },
  "notifications": {
    "desktop_enabled": true,
    "email_enabled": false,
    "default_reminder_minutes": 15,
    "high_priority_reminder_minutes": 30
  },
  "smtp": {
    "server": "",
    "port": 587,
    "username": "",
    "password": ""
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(levelname)s - %(message)s",
    "file_enabled": true
  },
  "nlp": {
    "spacy_model": "en_core_web_sm",
    "enable_advanced_parsing": true
  }
}
